VITE_API_BASE_URL=/api/v1
VITE_API_TIMEOUT_MS=30000

# Application Information
VITE_APP_TITLE=Molecular Data Management Platform
VITE_APP_DESCRIPTION=Platform for molecular data management and CRO integration

# Authentication Settings
VITE_TOKEN_EXPIRY_MINUTES=15
VITE_REFRESH_TOKEN_EXPIRY_DAYS=7

# Data Constraints
VITE_MAX_CSV_SIZE_MB=10
VITE_MAX_MOLECULES_DISPLAY=1000

# Feature Flags
VITE_ENABLE_MOCK_API=false
VITE_DEBUG_MODE=false
VITE_ENABLE_ERROR_BOUNDARIES=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# External Services
VITE_MINIO_ENDPOINT=/storage
VITE_WEBSOCKET_URL=/ws

# Development Tools
VITE_ENABLE_REACT_QUERY_DEVTOOLS=false
VITE_ENABLE_REDUX_DEVTOOLS=false

# Cache Settings
VITE_CACHE_TTL_MINUTES=15